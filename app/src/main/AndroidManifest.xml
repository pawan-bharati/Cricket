        </activity>

        <activity android:name=".Landind_Activity" >

        </activity>
        <activity android:name=".SplashScreen_Activity">
        </activity>
        <activity android:name=".Dashboard" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>